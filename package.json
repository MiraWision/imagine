{
  "name": "@mirawision/imagine",
  "version": "2.0.2",
  "description": "Deterministic random data generator for mocks, seeds, demos, and tests.",
  "keywords": [
    "faker",
    "random",
    "seed",
    "deterministic",
    "mock",
    "testing",
    "typescript",
    "esm",
    "svg",
    "data",
    "generator"
  ],
  "license": "MIT",
  "author": "Yelysei Lukin <yelysei277@gmail.com>",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && webpack --config webpack.config.cjs && npm run build:types",
    "build:types": "tsc --emitDeclarationOnly --outDir dist/types",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "jest --runInBand",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "pack": "npm run build && npm pack",
    "publish": "npm publish --access public"
  },
  "sideEffects": false,
  "main": "./dist/cjs/index/index.js",
  "module": "./dist/esm/index/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index/index.js",
      "require": "./dist/cjs/index/index.js",
      "default": "./dist/esm/index/index.js"
    },
    "./location": {
      "types": "./dist/types/location/index.d.ts",
      "import": "./dist/esm/location/index.js",
      "require": "./dist/cjs/location/index.js",
      "default": "./dist/esm/location/index.js"
    },
    "./number": {
      "types": "./dist/types/number/index.d.ts",
      "import": "./dist/esm/number/index.js",
      "require": "./dist/cjs/number/index.js",
      "default": "./dist/esm/number/index.js"
    },
    "./boolean": {
      "types": "./dist/types/boolean/index.d.ts",
      "import": "./dist/esm/boolean/index.js",
      "require": "./dist/cjs/boolean/index.js",
      "default": "./dist/esm/boolean/index.js"
    },
    "./id": {
      "types": "./dist/types/id/index.d.ts",
      "import": "./dist/esm/id/index.js",
      "require": "./dist/cjs/id/index.js",
      "default": "./dist/esm/id/index.js"
    },
    "./string": {
      "types": "./dist/types/string/index.d.ts",
      "import": "./dist/esm/string/index.js",
      "require": "./dist/cjs/string/index.js",
      "default": "./dist/esm/string/index.js"
    },
    "./util": {
      "types": "./dist/types/util/index.d.ts",
      "import": "./dist/esm/util/index.js",
      "require": "./dist/cjs/util/index.js",
      "default": "./dist/esm/util/index.js"
    },
    "./images": {
      "types": "./dist/types/images/index.d.ts",
      "import": "./dist/esm/images/index.js",
      "require": "./dist/cjs/images/index.js",
      "default": "./dist/esm/images/index.js"
    },
    "./person": {
      "types": "./dist/types/person/index.d.ts",
      "import": "./dist/esm/person/index.js",
      "require": "./dist/cjs/person/index.js",
      "default": "./dist/esm/person/index.js"
    },
    "./date": {
      "types": "./dist/types/date/index.d.ts",
      "import": "./dist/esm/date/index.js",
      "require": "./dist/cjs/date/index.js",
      "default": "./dist/esm/date/index.js"
    },
    "./internet": {
      "types": "./dist/types/internet/index.d.ts",
      "import": "./dist/esm/internet/index.js",
      "require": "./dist/cjs/internet/index.js",
      "default": "./dist/esm/internet/index.js"
    },
    "./color": {
      "types": "./dist/types/color/index.d.ts",
      "import": "./dist/esm/color/index.js",
      "require": "./dist/cjs/color/index.js",
      "default": "./dist/esm/color/index.js"
    },
    "./game": {
      "types": "./dist/types/game/index.d.ts",
      "import": "./dist/esm/game/index.js",
      "require": "./dist/cjs/game/index.js",
      "default": "./dist/esm/game/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MiraWision/imagine"
  },
  "bugs": {
    "url": "https://github.com/MiraWision/imagine/issues"
  },
  "homepage": "https://github.com/MiraWision/imagine#readme",
  "devDependencies": {
    "@types/jest": "29.5.12",
    "@types/node": "20.11.30",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "jest": "29.7.0",
    "prettier": "3.3.1",
    "rimraf": "5.0.5",
    "ts-jest": "29.1.2",
    "typescript": "5.4.5",
    "ts-loader": "9.5.1",
    "webpack": "5.92.1",
    "webpack-cli": "5.1.4"
  },
  "peerDependencies": {
    "@mirawision/infopedia": ">=1.0.0"
  }
}

